uses "./Scanner.llld";

Program
    : {Statement [";"]}
    ;

Statement
    : "let" LowerID "=" Expression
    | "print" "(" [Expression {"," Expression}] ")"
    | "println" "(" [Expression {"," Expression}] ")"
    ;

Expression
    : Factor
    ;

Factor
    : "(" Expression ")"
    | LiteralInt
    | LowerID
    ;
